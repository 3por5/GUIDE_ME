<div class="container">
  <%= form_with(model: program) do |f| %>
      <%= f.label :name, style: "display: block" %>
      <%= f.text_field :name %>

      <%= f.label :description, style: "display: block" %>
      <%= f.text_field :description %>

      <%= f.label :category, style: "display: block" %>
      <%= f.select :category, %w[Ruby JS CSS PMI Produtividade C# Empregos Exterior Python Mindset] %>

      <div><%= f.submit %></div>
  <% end %>
</div>
